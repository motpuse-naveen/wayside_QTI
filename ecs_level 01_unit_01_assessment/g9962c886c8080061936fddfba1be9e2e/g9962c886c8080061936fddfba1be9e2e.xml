            <render_choice>
              <response_label ident="65807">
                <material>
                  <mattext texttype="text/plain">Buenas noches.</mattext>
                </material>
              </response_label>
              <response_label ident="23793">
                <material>
                  <mattext texttype="text/plain">Hasta luego.</mattext>
                </material>
              </response_label>
              <response_label ident="9368">
                <material>
                  <mattext texttype="text/plain">Hola.</mattext>
