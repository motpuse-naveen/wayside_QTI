          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="97082">
                <material>
                  <mattext texttype="text/plain">marcha</mattext>
                </material>
              </response_label>
              <response_label ident="94841">
