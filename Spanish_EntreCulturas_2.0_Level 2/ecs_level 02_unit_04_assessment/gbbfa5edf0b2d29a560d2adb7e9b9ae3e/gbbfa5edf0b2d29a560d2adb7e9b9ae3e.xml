        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;p&gt;Look at the pictures and choose the most appropriate word to complete the sentence.&lt;/p&gt;
       

          &lt;p&gt;1. &lt;/p&gt;&lt;p&gt;&lt;img src="https://cdn.waysidepublishing.com/textbooks/QTI/ECS2/ecs2_u4_ua_vq1_activity1_q1.jpg" alt="a woman tanning at the beach" style="width: 50%; border: 1px solid #808080;"&gt;&lt;/p&gt;

          &lt;p&gt;Deila fue a las playas de Ecuador para ________.&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="20583">
                <material>
                  <mattext texttype="text/plain">tomar el sol</mattext>
                </material>
              </response_label>
              <response_label ident="58788">
                <material>
                  <mattext texttype="text/plain">sacar unas fotos</mattext>
                </material>
              </response_label>
              <response_label ident="97832">
                <material>
                  <mattext texttype="text/plain">observar las estrellas</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">20583</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
