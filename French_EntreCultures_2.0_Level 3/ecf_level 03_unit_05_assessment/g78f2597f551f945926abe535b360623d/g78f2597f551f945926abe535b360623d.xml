            <mattext texttype="text/html">&lt;div&gt;2. Quelle influence l’environnement peut-il avoir sur l’identité ?&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="27603">
                <material>
                  <mattext texttype="text/html">l'environnement n’a pas d’influence sur l'identité</mattext>
                </material>
              </response_label>
              <response_label ident="46937">
                <material>
                  <mattext texttype="text/plain">il peut guider la manière dont les élèves voient le monde</mattext>
                </material>
              </response_label>
              <response_label ident="23788">
                <material>
                  <mattext texttype="text/plain">il détermine l’avenir d’une personne</mattext>
                </material>
