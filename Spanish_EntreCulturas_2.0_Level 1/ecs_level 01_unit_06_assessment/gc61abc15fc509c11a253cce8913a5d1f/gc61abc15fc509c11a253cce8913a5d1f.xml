            &lt;p&gt;En ______ hay muchos animales.&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="56579">
                <material>
                  <mattext texttype="text/plain">la costa.</mattext>
                </material>
              </response_label>
              <response_label ident="9322">
                <material>
                  <mattext texttype="text/plain">la selva</mattext>
                </material>
              </response_label>
              <response_label ident="5801">
                <material>
                  <mattext texttype="text/plain">el desierto</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
