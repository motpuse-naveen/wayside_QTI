        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;p&gt;Read each scenario and choose the correct phrase that you would say to the given person at the specified time of the day.&lt;/p&gt;
        &lt;p&gt;08:00 a.m.&lt;i&gt; What you would say to your teacher?&lt;/i&gt;&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="74807">
                <material>
                  <mattext texttype="text/plain">Buenas tardes.</mattext>
                </material>
              </response_label>
              <response_label ident="11882">
                <material>
                  <mattext texttype="text/plain">Hola. ¿Cómo estás?</mattext>
                </material>
              </response_label>
              <response_label ident="76134">
                <material>
                  <mattext texttype="text/plain">Buenos días. ¿Cómo está usted?</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
