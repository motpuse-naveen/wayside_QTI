            <mattext texttype="text/html">&lt;div&gt;&lt;p&gt;2. Alina est allergique aux produits laitiers. Est-ce qu'elle mange du fromage de temps en temps ?&lt;/p&gt;&lt;p style="margin-top:10px;"&gt;Non,&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_str ident="response1" rcardinality="Single">
            <render_fib>
              <response_label ident="answer1" rshuffle="No"/>
            </render_fib>
          </response_str>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">Alina ne mange pas de fromage.</varequal>
              <varequal respident="response1">Alina ne mange jamais de fromage.</varequal>
              <varequal respident="response1">elle ne mange pas de fromage.</varequal>
              <varequal respident="response1">elle ne mange jamais de fromage.</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
