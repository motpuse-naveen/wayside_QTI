      <item ident="ge65001f6fe9a29e6672cb728edf54114" title="Write an essay">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>short_answer_question</fieldentry>
