        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;p&gt;Review the Belgian calendar provided and select the written date that corresponds with the numerical date given (in the francophone format).&lt;/p&gt;

           &lt;p&gt;&lt;img src="https://cdn.waysidepublishing.com/textbooks/QTI/ECF1/ecf1_u0_vgq_vq3_activity1_ex.jpg" alt="Calendar in grid format written in French for a full unspecified non-leap year with the first of January falling on a Friday. The days of the week start on Monday with Sundays written in red." style="width: 80%; margin-left: 66px; border: solid black 1px;"&gt;&lt;/p&gt;
    

         &lt;p&gt;05/04 ____________.&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="70198">
                <material>
                  <mattext texttype="text/plain">mardi quatre mai</mattext>
                </material>
              </response_label>
              <response_label ident="76462">
                <material>
                  <mattext texttype="text/plain">dimanche quatre avril</mattext>
                </material>
              </response_label>
              <response_label ident="63027">
                <material>
                  <mattext texttype="text/plain">lundi cinq avril</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">63027</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
